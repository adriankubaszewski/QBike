<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.qbike.PrzejechanetrasyActivity">

    <ListView
        android:id="@+id/listViewTrasy"
        android:layout_width="0dp"
        android:layout_height="370dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="3dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="3dp"
        android:layout_marginRight="3dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="105dp"
        android:layout_height="42dp"
        android:text="Przejechany dystans:"
        android:textSize="18dp"
        android:layout_marginStart="16dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginEnd="22dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textViewWypiszDystans"
        app:layout_constraintRight_toRightOf="@+id/textViewWypiszDystans"
        android:layout_marginBottom="1dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="22dp"
        app:layout_constraintHorizontal_bias="0.0"
        tools:layout_editor_absoluteY="3dp" />

    <TextView
        android:id="@+id/textViewWypiszDystans"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="0,0 km"
        android:textSize="18dp"
        android:layout_marginStart="32dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintRight_toLeftOf="@+id/textViewWypiszLiczbeTras"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/textViewWypiszLiczbeTras"
        android:layout_marginEnd="31dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textViewWypiszLiczbeTras"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="31dp" />
    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:text="Przejechane trasy:"
        android:textSize="18dp"
        android:layout_marginStart="10dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textViewWypiszLiczbeTras"
        android:layout_marginEnd="12dp"
        app:layout_constraintRight_toRightOf="@+id/listViewTrasy"
        android:layout_marginBottom="7dp"
        app:layout_constraintLeft_toRightOf="@+id/textViewWypiszDystans"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="12dp"
        app:layout_constraintHorizontal_bias="0.0" />
    <TextView
        android:id="@+id/textViewWypiszLiczbeTras"
        android:layout_width="95dp"
        android:layout_height="22dp"
        android:text="0"
        android:textSize="18dp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/listViewTrasy"
        android:layout_marginEnd="45dp"
        app:layout_constraintRight_toRightOf="@+id/listViewTrasy"
        android:layout_marginBottom="7dp"
        android:layout_marginRight="46dp" />
</android.support.constraint.ConstraintLayout>
